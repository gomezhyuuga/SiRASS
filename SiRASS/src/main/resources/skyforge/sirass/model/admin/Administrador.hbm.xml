<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="skyforge.sirass.model.admin.Administrador" table="MAdministrador">
      <id name="idAdmin" column="idAdmin" type="integer">
          <generator class="increment" />
      </id>
      <property name="nombre" column="nombre" type="java.lang.String" length="25" not-null="false" />
      <property name="aPaterno" column="aPaterno" type="java.lang.String" length="15" not-null="false" />
      <property name="aMaterno" column="aMaterno" type="java.lang.String" length="15" not-null="false" />
      <property name="email" column="email" type="java.lang.String" length="30" not-null="false" />
      <property name="nacimiento" column="nacimiento" type="date" not-null="false" />
      <property name="sexo" column="sexo" type="char" length="25" not-null="false" />
      <property name="cargo" column="cargo" type="java.lang.String" length="100" not-null="false" />
      <property name="creacion" column="creacion" type="java.util.Date" not-null="false" />
      <property name="modificadoPor" column="modificadoPor" type="java.lang.String" length="15" not-null="false" />
      <property name="ultimaModif" column="ultimaModif" type="java.util.Date" not-null="false" />
  </class>
</hibernate-mapping>

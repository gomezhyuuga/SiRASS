<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="skyforge.sirass.model.programass.DTipoPrograma" table="DTipoPrograma">
        <composite-id>
            <key-property column="idPrograma" name="idPrograma" type="integer">
            </key-property>
            <key-property column="idTipo" name="idTipo" type="integer">
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
